---
import type { Props as BaseHeadProps } from "../components/BaseHead.astro";
import Layout from "./Base.astro";
import SideLeftPage from "../components/SideLeftPage.astro";
import SideRightPage from "../components/SideRightPage.astro";
import Header from "../components/Header.astro";
// import Footer from '../components/Footer.astro';

export interface Props {
  seo?: BaseHeadProps;
}

const { seo } = Astro.props as Props;
---

<Layout {seo}>
  <Header slot="header" />
  <SideLeftPage class="aside aside-1" slot="side-left" />
  <slot />
  <SideRightPage class="aside aside-2" slot="side-right" />
  <!-- <Footer slot="footer" /> -->
</Layout>
